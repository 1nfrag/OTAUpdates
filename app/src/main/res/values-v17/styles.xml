<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="FabStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/fab_button_margin_right</item>
        <item name="android:layout_marginEnd">@dimen/fab_button_margin_right</item>
    </style>

    <style name="UpdateIconTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Body1</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_marginLeft">@dimen/update_icon_type_offset</item>
        <item name="android:layout_marginStart">@dimen/update_icon_type_offset</item>
    </style>

</resources>
